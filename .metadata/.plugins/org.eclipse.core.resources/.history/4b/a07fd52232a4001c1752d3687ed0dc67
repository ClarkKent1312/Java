package com.capg.work;

public class Test {
	public static String calcGST(String newString){
	      String[] s = newString.split("@");
	      //int GST = 0;
	      
	      for (int i = 0; i< 6 ; i++){
	        String[] s1 = s[i].split(",");
	        System.out.println(s1[i]);
	      }
	      
	      
	      String result = "";
	      return result;
	    }
	
	public static void main(String[] args) {
		Test.calcGST("100,20@70,30@40,50");
	}
	
	
	
	
}
